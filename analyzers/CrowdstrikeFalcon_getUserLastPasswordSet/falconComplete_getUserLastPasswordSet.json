{
    "name": "CrowdstrikeFalcon_getUserLastPasswordSet",
    "version": "1.0",
    "author": "John Requejo",
    "url": "https://johnrequejo.com",
    "license": "AGPL-v3",
    "description": "Retrieves all the information found for observable of type username such as the last password update date.",
    "dataTypeList": ["username"],
    "baseConfig": "CrowdstrikeFalcon_getUserLastPasswordSet",
    "command": "CrowdstrikeFalcon/falconComplete_getUserLastPasswordSet.py",
    "configurationItems": [
      {
        "name": "clientSecret",
        "description": "Your FalconComplete clientSecret.",
        "type": "string",
        "multi": false,
        "required": true
      },
      {
        "name": "clientId",
        "description": "Your FalconComplete clientId.",
        "type": "string",
        "multi": false,
        "required": true
      },
      {
        "name": "targetHostName",
        "description": "Your AD hostName.",
        "type": "string",
        "multi": false,
        "required": true
      },
      {
        "name": "outputFile",
        "description": "The full path where the result would be temporaly save.",
        "type": "string",
        "multi": false,
        "required": true,
        "default": "C:\\temp\\PasswordSet.txt"
      }
    ],
    "config": {
        "auto_extract": false 
    }
}
